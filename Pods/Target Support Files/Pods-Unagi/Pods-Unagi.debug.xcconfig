EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/EZLoadingActivity" "$PODS_CONFIGURATION_BUILD_DIR/Hue" "$PODS_CONFIGURATION_BUILD_DIR/LTMorphingLabel" "$PODS_CONFIGURATION_BUILD_DIR/ZFRippleButton"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/EZLoadingActivity/EZLoadingActivity.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Hue/Hue.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/LTMorphingLabel/LTMorphingLabel.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/ZFRippleButton/ZFRippleButton.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "EZLoadingActivity" -framework "Hue" -framework "LTMorphingLabel" -framework "ZFRippleButton"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
